{
  "name": "googlebooksearch",
  "version": "1.0.0",
  "description": "Google Books Search",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "author": "Alexis C.",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^4.1.1",
    "nodemon": "^1.19.1"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "handlebars": "^4.1.2",
    "handlebars-react": "0.0.16",
    "if-env": "^1.0.4",
    "js-yaml": "^3.13.1",
    "mongoose": "^5.6.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/4l3x1s/GoogleBooksSearch.git"
  },
  "bugs": {
    "url": "https://github.com/4l3x1s/GoogleBooksSearch/issues"
  },
  "homepage": "https://github.com/4l3x1s/GoogleBooksSearch#readme",
  "optionalDependencies": {}
}
